{
  "version": "0.1.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && echo '#!/usr/bin/env node\\n' \"$(cat dist/index.js)\" > dist/index.js",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build && echo '#!/usr/bin/env node\\n' \"$(cat dist/index.js)\" > dist/index.js",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "dodao-course-processor",
  "author": "robin",
  "bin": {
    "dodao-course-processor": "dist/index.js"
  },
  "module": "dist/course-processor.esm.js",
  "size-limit": [
    {
      "path": "dist/course-processor.cjs.production.min.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/course-processor.esm.js",
      "limit": "100 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jsonschema": "^1.1.1",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "commander": "^9.3.0",
    "jsonschema": "^1.4.1",
    "yaml": "^2.1.1"
  }
}
